{
    "name": "wttks/laravel-jlg-code",
    "description": "Japan Local Government Code (JLG) support for Laravel: prefectures, municipalities, address parsing",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Wttks\\JlgCode\\": "src/",
            "Wttks\\JlgCode\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wttks\\JlgCode\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wttks\\JlgCode\\JlgCodeServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
